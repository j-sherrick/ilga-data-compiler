{
   "compilerOptions": {
      "module": "nodenext",
      "target": "es2020",
      "baseUrl": "./src",
      "paths": {
         "@interfaces/*": ["schemas/ILCS/interfaces/*"],
         "@models/*": ["schemas/ILCS/models/*"],
         "@services/*": ["services/ILCS/*"]
      },
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true
   },
   "files": ["tests/unit/extractorTest.ts"],
   "exclude": ["node_modules"]
}
