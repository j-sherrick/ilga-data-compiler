{
   "compilerOptions": {
      "module": "nodenext",
      "target": "es2020",
      "baseUrl": "./src",
      "paths": {
         "@interfaces/*": ["models/ILCS/interfaces/*"],
         "@models/*": ["models/ILCS/models/*"],
         "@utlils/*": ["utils/ILCS/*"]
      },
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true
   },
   "files": ["tests/unit/extractorTest.ts", "src/utils/ILCS/parser.ts"],
   "exclude": ["node_modules"]
}
